{
  "name": "nuxt-auth-toolkit",
  "version": "1.0.0",
  "description": "nuxt auth toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devcui/nuxt-auth-toolkit.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "pre:publish:github":"node ./scripts/github-package.js",
    "clean": "node scripts/rm.js .nuxt dist playground/.nuxt",
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "changelogen --release --push",
    "lint": "eslint . --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "cookie-es": "^1.1.0",
    "dayjs": "^1.11.11",
    "defu": "^6.1.4",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "next-auth": "^4.24.7",
    "ofetch": "^1.3.4",
    "pinia": "^2.1.7",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.7.1",
    "@nuxt/schema": "^3.12.2",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.2",
    "changelogen": "^0.5.5",
    "eslint": "^9.5.0",
    "nuxt": "^3.12.2",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
